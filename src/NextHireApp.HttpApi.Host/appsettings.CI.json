{
  "App": {
    "CorsOrigins": "http://localhost:4200,http://localhost:44307,https://localhost:44307"
  },
  "ConnectionStrings": {
    "Default": "Data Source=minhquan\\SQLEXPRESS;Initial Catalog=NextHire;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "CAP": {
    "DefaultGroupName": "your_default_consumer",
    "Group": "your_default_consumer",
    "ConsumerThreadCount": 3,
    "Kafka": {
      "Connections": {
        "Default": {
          "BootstrapServers": ""
        }
      },
      "AutoCreateTopics": "false",
      "AutoOffsetReset": "latest"
    }
  },
  "Redis": {
    "Configuration": ""
  },
  "Elasticsearch": {
    "Indices": {
      "CustomIndex": "your_index_name"
    },
    "Url": "",
    "Username": "",
    "Password": ""
  },
  "RemoteServices": {
    "OMSAPI": {
      "BaseUrl": "your-uri"
    }
  },
  "AbpAuditingOptions": {
    "IsEnabled": true,
    "HideErrors": true,
    "IsEnabledForAnonymousUsers": true,
    "AlwaysLogOnException": true,
    "IsEnabledForGetRequests": true,
    "ApplicationName": "Name Of Application",
    "IsEnabledLogResponse": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "DotNetCore.CAP": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/.log",
          "rollingInterval": "Hour",
          "retainedFileCountLimit": 1440
        }
      },
